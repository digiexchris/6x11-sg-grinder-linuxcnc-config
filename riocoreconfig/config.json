{
    "name": "TangNano9K",
    "boardcfg": "TangNano9K",
    "toolchain": "icestorm",
    "plugins": [
        {
            "type": "spi",
            "pins": {
                "mosi": {
                    "pin": "25"
                },
                "miso": {
                    "pin": "26"
                },
                "sclk": {
                    "pin": "27"
                },
                "sel": {
                    "pin": "28"
                }
            }
        },
        {
            "type": "freqin",
            "pins": {
                "freq": {
                    "pin": "29",
                    "pull": "down"
                }
            },
            "signals": {
                "frequency": {
                    "net": "spindle.0.revs"
                },
                "valid": {
                    "net": "spindle.0.at-speed"
                }
            },
            "freq_min": 5,
            "name": "SPINDLE_RPM_IN"
        },
        {
            "type": "pwmout",
            "pins": {
                "pwm": {
                    "pin": "30"
                }
            },
            "signals": {
                "dty": {
                    "net": "spindle.0.speed-out"
                },
                "enable": {
                    "net": "spindle.0.on"
                }
            },
            "name": "SPINDLE_SPEED_PWM_OUT"
        },
        {
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "40"
                },
                "dir": {
                    "pin": "35"
                },
                "en": {
                    "pin": "41"
                }
            },
            "name": "ZStepper",
            "axis": "Z",
            "is_joint": true
        },
        {
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "42"
                },
                "dir": {
                    "pin": "51"
                },
                "en": {
                    "pin": "53"
                }
            },
            "name": "YStepper",
            "axis": "Y",
            "is_joint": true
        },
        {
            "type": "stepdir",
            "pins": {
                "step": {
                    "pin": "54"
                },
                "dir": {
                    "pin": "55"
                },
                "en": {
                    "pin": "56"
                }
            },
            "name": "XStepper",
            "axis": "X",
            "is_joint": true
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "57",
                    "pull": "up"
                }
            },
            "name": "ESTOP"
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "77",
                    "pull": "up"
                }
            },
            "signals": {
                "bit": {
                    "net": "joint.3.home-sw-in"
                }
            },
            "name": "ZHOME"
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "76",
                    "pull": "up"
                }
            },
            "signals": {
                "bit": {
                    "net": "joint.1.home-sw-in"
                }
            },
            "name": "YHOME"
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "75",
                    "pull": "up"
                }
            },
            "signals": {
                "bit": {
                    "net": "joint.2.home-sw-in"
                }
            },
            "name": "XHOME"
        }
    ]
}